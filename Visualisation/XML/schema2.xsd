<!-- Marked Up -->
<xs:element name="markedUp" type="xs:string"/>
<xs:element name="extracted_html" type="xs:string"/>
<xs:element name="mime_type" type="xs:string"/>

<!-- Reasoning -->
<xs:element name="reasoning" type="xs:string"/>
<xs:element name="reasoning_86" type="xs:string"/>
<xs:element name="marker" type="xs:string"/>
<xs:element name="marker_tuple" type="xs:string"/>
<xs:element name="ngrams_count" type="xs:integer"/>

<!-- Evidence -->
<xs:element name="evidence" type="xs:string"/>
<xs:element name="experience_9" type="xs:string"/>
<xs:element name="marker" type="xs:string"/>
<xs:element name="marker_tuple" type="xs:string"/>
<xs:element name="ngrams_count" type="xs:integer"/>
<xs:element name="nltk_named_entities" type="xs:string"/>
<xs:element name="pronouns" type="xs:string"/>
<xs:element name="timex-events" type="xs:string"/>
<xs:element name="verb-events" type="xs:string"/>
<xs:element name="verb" type="xs:string"/>
<xs:element name="verbType" type="xs:string"/>

<!-- Citations -->
<xs:element name="citations" type="xs:string"/>
<xs:element name="all_uris" type="xs:string"/>
<xs:element name="external_uris" type="xs:string"/>
<xs:element name="classified_external_uris" type="xs:string"/>
<xs:element name="uri" type="xs:string"/>
<xs:element name="domain" type="xs:string"/>
<xs:element name="classifications" type="xs:string"/>
<xs:element name="evidence_type" type="xs:string"/>
<xs:element name="evidence_name" type="xs:string"/>

<!-- definition of attributes -->
<xs:attribute name="greyLiterature" type="xs:string"/>

<!-- definition of complex elements -->
<xs:element name="markedUp">
    <xs:complexType>
        <xs:sequence>
            <xs:element ref="extracted_html"/>
            <xs:element ref="mime_type"/>
        </xs:sequence>
    </xs:complexType>
</xs:element>

<xs:element name="reasoning">
    <xs:complexType>
        <xs:sequence>
            <xs:element ref="reasoning_86"/>
            <xs:element ref="marker"/>
            <xs:element ref="marker_tuple"/>
            <xs:element ref="ngrams_count"/>
        </xs:sequence>
    </xs:complexType>
</xs:element>

<xs:element name="experience">
    <xs:complexType>
        <xs:sequence>
            <xs:element ref="titexperience_9le"/>
            <xs:element ref="marker"/>
            <xs:element ref="marker_tuple"/>
            <xs:element ref="ngrams_count"/>
            <xs:element ref="nltk_named_entities"/>
            <xs:element ref="pronouns"/>
            <xs:element ref="timex-events"/>
            <xs:element ref="verb-events"/>
            <xs:element ref="verb"/>
            <xs:element ref="verbType"/>
        </xs:sequence>
    </xs:complexType>
</xs:element>

<xs:element name="citations">
    <xs:complexType>
        <xs:sequence>
            <xs:element ref="all_uris"/>
            <xs:element ref="external_uris"/>
            <xs:element ref="external_uris"/>
            <xs:element ref="external_uris"/>
            <xs:element ref="external_uris"/>
            <xs:element ref="classified_external_uris"/>
            <xs:element ref="uri"/>
            <xs:element ref="domain"/>
            <xs:element ref="classifications"/>
            <xs:element ref="evidence_type"/>
            <xs:element ref="evidence_name"/>
        </xs:sequence>
        <xs:attribute ref="greyLiterature" use="required"/>
    </xs:complexType>
</xs:element>
